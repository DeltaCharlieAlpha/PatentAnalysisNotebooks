   "source": [
    "#Fit the data\n",
    "print(\"Grid Searching\")\n",
    "text_clf.fit( features, labels)\n",
    "print(\"Best Score\", grid_search.best_score_)\n",
    "print( \"Best parameters set:\" )\n",
    "best_parameters = grid_search.best_estimator_.get_params()\n",
