   "source": [
    "# Query BigQuery to get training text"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Requesting query... ok.\n",
      "Job ID: job_u2uDVXzTh_SgLsACIsao5OIJXhfp\n",
